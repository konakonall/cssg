{
  "docExtension": ".md",

  "commentDelimiter": ["//", ""],
  "sourceFileNamingConvention": "upper-camel",
  "methodNamingConvention": "lower-camel",
  "classNamingConvention": "upper-camel",

  "langs": {
    "android": {
      "sourceExtension": ".java"
    },
    "objc": {
      "snippetNameCommonPrefix": "objc-",
      "sourceExtension": ".m"
    },
    "swift": {
      "snippetNameCommonPrefix": "swift-",
      "sourceExtension": ".swift"
    },
    "java": {
      "sourceExtension": ".java"
    },
    "go": {
      "sourceExtension": ".go",
      "sourceFileNamingConvention": "under-score",
      "methodNamingConvention": "upper-camel"
    },
    "dotnet": {
      "sourceExtension": ".cs"
    },
    "node": {
      "sourceExtension": ".js"
    },
    "js": {
      "sourceExtension": ".js"
    },
    "php": {
      "sourceExtension": ".php"
    },
    "python": {
      "commentDelimiter": ["#", ""],
      "sourceExtension": ".py"
    }
  },

  "macro4example": {
    "secretId": "COS_SECRETID",
    "secretKey": "COS_SECRETKEY",

    "appId": "1250000000",
    "uin": "100000000001",
    "region": "COS_REGION",
    "bucket": "examplebucket-",
    "copySourceBucket": "sourcebucket-",
    "replicationDestBucket":"destinationbucket-",
    "object": "exampleobject",

    "eTag": "exampleETag",
    "uploadId": "exampleUploadId"
  },
  "macro4test": {
    "appId": "1253653367",
    "uin": "1278687956",
    "region": "ap-guangzhou",
    "bucket": "bucket-cssg-test-${config.language}-",
    "copySourceBucket":"bucket-cssg-source-",
    "replicationDestBucket":"bucket-cssg-assist-",
    "object": "object4${config.language}"
  },

  "groups": {
    "bucket": [
      "put-bucket",
      "head-bucket",
      "delete-bucket"
    ],

    "list-Object": [
      "get-bucket",
      "list-object-versioning"
    ],
    
    "bucket-ACL": [
      "put-bucket-acl",
      "get-bucket-acl"
    ],
    "bucket-policy": [
      "put-bucket-policy",
      "get-bucket-policy",
      "delete-bucket-policy"
    ],
    "bucket-CORS": [
      "put-bucket-cors",
      "get-bucket-cors",
      "option-object",
      "delete-bucket-cors"
    ],
    "bucket-lifecycle": [
